{% block title %}Society
{% endblock %}

{% block body %}

	<h1>Society</h1>

	<table>
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ society.id }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ society.name }}</td>
			</tr>
